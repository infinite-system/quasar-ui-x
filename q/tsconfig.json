{
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "moduleResolution": "node",
        "strict": true,
        "jsx": "preserve",
        "sourceMap": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "lib": [
            "esnext",
            "dom",
        ],
        "types": [
            "vite/client",
        ],
        "baseUrl": ".",
      "paths": {
        "src/*": [
          "src/*"
        ],
        "app/*": [
          "*"
        ],
        "components/*": [
          "src/components/*"
        ],
        "layouts/*": [
          "src/layouts/*"
        ],
        "pages/*": [
          "src/pages/*"
        ],
        "assets/*": [
          "src/assets/*"
        ],
        "boot/*": [
          "src/boot/*"
        ],
        "stores/*": [
          "src/stores/*"
        ],
        "vue$": [
          "node_modules/vue/dist/vue.runtime.esm-bundler.js"
        ]
      }
    },
    "include": [
        "src/**/*"
    ]
}
